{
  "name": "matukotoBlog",
  "type": "module",
  "version": "1.0.0",
  "license": "WTFPL",
  "packageManager": "pnpm@10.19.0",
  "scripts": {
    "build": "run-s tsc urara:build kit:build clean",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "clean": "node urara.js clean",
    "dev": "run-s tsc \"dev:parallel {@} \" --",
    "dev:parallel": "run-p -r tsc:watch urara:watch \"kit:dev {@} \" --",
    "kit:build": "cross-env NODE_OPTIONS=--max_old_space_size=7680 vite build",
    "kit:dev": "cross-env NODE_OPTIONS=--max_old_space_size=7680 vite dev",
    "lint": "eslint --flag unstable_ts_config .",
    "lint:fix": "eslint --flag unstable_ts_config . --fix",
    "preview": "vite preview",
    "start": "cross-env ADAPTER=node pnpm build && node build",
    "tsc": "tsc -p tsconfig.node.json",
    "tsc:watch": "tsc -w -p tsconfig.node.json",
    "urara:build": "node urara.js build",
    "urara:watch": "node urara.js watch"
  },
  "devDependencies": {
    "@antfu/eslint-config": "6.0.0",
    "@iconify-json/heroicons-outline": "1.2.1",
    "@iconify-json/heroicons-solid": "1.2.1",
    "@importantimport/eslint-config": "1.0.0-beta.2",
    "@sveltejs/adapter-cloudflare": "4.9.0",
    "@sveltejs/kit": "2.47.3",
    "@sveltejs/vite-plugin-svelte": "3.1.2",
    "@tailwindcss/typography": "0.5.19",
    "@types/node": "22.18.12",
    "@types/unist": "3.0.3",
    "@unocss/eslint-plugin": "66.5.4",
    "@unocss/extractor-svelte": "66.5.4",
    "chalk": "5.6.2",
    "chokidar": "4.0.1",
    "cross-env": "10.1.0",
    "daisyui": "4.12.24",
    "eslint": "9.38.0",
    "eslint-plugin-svelte": "3.12.5",
    "fenceparser": "2.2.0",
    "fff-flavored-frontmatter": "1.2.1",
    "github-slugger": "2.0.0",
    "mdast-util-to-string": "4.0.0",
    "mdsvex": "0.12.3",
    "npm-run-all2": "8.0.4",
    "postcss": "8.5.6",
    "postcss-lightningcss": "1.0.2",
    "rehype-autolink-headings": "7.1.0",
    "rehype-external-links": "3.0.0",
    "rehype-slug": "6.0.0",
    "remark": "14.0.3",
    "remark-fff": "1.2.1",
    "remark-footnotes": "2.0.0",
    "shiki-twoslash": "3.1.2",
    "svelte": "4.2.20",
    "svelte-check": "3.8.6",
    "svelte-eslint-parser": "1.4.0",
    "svelte-preprocess": "6.0.3",
    "sveltekit-embed": "0.0.22",
    "tailwindcss": "3.4.18",
    "tslib": "2.8.1",
    "typescript": "5.9.3",
    "unist-util-visit": "5.0.0",
    "unocss": "66.5.4",
    "vite": "6.4.1",
    "vite-imagetools": "9.0.0",
    "workbox-build": "7.3.0",
    "workbox-window": "7.3.0",
    "@textlint/textlint-plugin-markdown": "15.2.3",
    "@textlint/textlint-plugin-text": "15.2.3",
    "textlint": "15.2.3",
    "textlint-rule-ja-space-between-half-and-full-width": "2.4.2",
    "textlint-rule-preset-ja-spacing": "2.4.3",
    "textlint-rule-preset-ja-technical-writing": "12.0.2"
  },
  "dependencies": {
    "@iconify-json/simple-icons": "^1.2.13"
  }
}
