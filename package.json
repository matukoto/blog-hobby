{
  "name": "matukotoBlog",
  "type": "module",
  "version": "1.0.0",
  "license": "WTFPL",
  "packageManager": "pnpm@9.14.4",
  "scripts": {
    "build": "run-s tsc urara:build kit:build clean",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "clean": "node urara.js clean",
    "dev": "run-s tsc \"dev:parallel {@} \" --",
    "dev:parallel": "run-p -r tsc:watch urara:watch \"kit:dev {@} \" --",
    "kit:build": "cross-env NODE_OPTIONS=--max_old_space_size=7680 vite build",
    "kit:dev": "cross-env NODE_OPTIONS=--max_old_space_size=7680 vite dev",
    "lint": "eslint --flag unstable_ts_config .",
    "lint:fix": "eslint --flag unstable_ts_config . --fix",
    "preview": "vite preview",
    "start": "cross-env ADAPTER=node pnpm build && node build",
    "tsc": "tsc -p tsconfig.node.json",
    "tsc:watch": "tsc -w -p tsconfig.node.json",
    "urara:build": "node urara.js build",
    "urara:watch": "node urara.js watch"
  },
  "devDependencies": {
    "@antfu/eslint-config": "3.16.0",
    "@iconify-json/heroicons-outline": "1.2.1",
    "@iconify-json/heroicons-solid": "1.2.1",
    "@importantimport/eslint-config": "1.0.0-beta.2",
    "@sveltejs/adapter-cloudflare": "4.8.0",
    "@sveltejs/kit": "2.9.0",
    "@sveltejs/vite-plugin-svelte": "3.1.2",
    "@tailwindcss/typography": "0.5.16",
    "@types/node": "20.17.25",
    "@types/unist": "3.0.3",
    "@unocss/eslint-plugin": "0.65.4",
    "@unocss/extractor-svelte": "0.65.4",
    "chalk": "5.4.1",
    "chokidar": "4.0.1",
    "cross-env": "7.0.3",
    "daisyui": "4.12.24",
    "eslint": "9.23.0",
    "eslint-plugin-svelte": "2.46.1",
    "fenceparser": "2.2.0",
    "fff-flavored-frontmatter": "1.2.1",
    "github-slugger": "2.0.0",
    "mdast-util-to-string": "4.0.0",
    "mdsvex": "0.12.3",
    "npm-run-all2": "7.0.2",
    "postcss": "8.5.3",
    "postcss-lightningcss": "1.0.1",
    "rehype-autolink-headings": "7.1.0",
    "rehype-external-links": "3.0.0",
    "rehype-slug": "5.1.0",
    "remark": "14.0.3",
    "remark-fff": "1.0.0-alpha.1",
    "remark-footnotes": "2.0.0",
    "shiki-twoslash": "3.1.2",
    "svelte": "4.2.19",
    "svelte-check": "3.8.6",
    "svelte-eslint-parser": "0.43.0",
    "svelte-preprocess": "5.1.4",
    "sveltekit-embed": "0.0.20",
    "tailwindcss": "3.4.17",
    "tslib": "2.8.1",
    "typescript": "5.7.2",
    "unist-util-visit": "4.1.2",
    "unocss": "0.65.4",
    "vite": "6.2.2",
    "vite-imagetools": "7.0.5",
    "workbox-build": "7.3.0",
    "workbox-window": "7.3.0",
    "@textlint/textlint-plugin-markdown": "14.5.0",
    "@textlint/textlint-plugin-text": "14.5.0",
    "textlint": "14.5.0",
    "textlint-rule-ja-space-between-half-and-full-width": "2.4.2",
    "textlint-rule-preset-ja-spacing": "2.4.3",
    "textlint-rule-preset-ja-technical-writing": "12.0.2"
  },
  "dependencies": {
    "@iconify-json/simple-icons": "^1.2.13"
  }
}
